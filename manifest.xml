<manifest>
    <main-class>com.codelanx.aether.bots.cooking.CookingBot</main-class>
    <name>Aether Bot</name>
    <tag-line>Maybe a construction bot. Maybe not.</tag-line><!--Max of 50 chars-->
    <description>hoists my shitcode while I navigate the sea of runemate docs</description><!--Max of 110 chars-->
    <version>0.0.1</version>
    <compatibility>
        <game>OSRS</game>
        <!--game>RS3</game-->
        <!--Can be used if you absolutely need to disable DarkScape (highly discouraged)-->
        <!--<disable-game>DARK</disable-game>-->
    </compatibility>
    <categories>
        <category>CONSTRUCTION</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>INVALID</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>Spaghetti</tag>
    </tags>
    <resources>
        <resource>src/main/resources</resource>
    </resources>
</manifest>

