<!-- here and below is just for the example -->
<manifest>
    <main-class>com.example.mahbots.prayer.BoneBot</main-class>
    <name>Bone Bot</name>
    <tag-line>Thought it was BoneBot, keeps picking up bananas</tag-line><!--Max of 50 chars-->
    <description>An example bot manifest for running with maven</description><!--Max of 110 chars-->
    <version>0.0.1</version>
    <compatibility>
        <game>OSRS</game>
        <!--game>RS3</game-->
        <!--Can be used if you absolutely need to disable DarkScape (highly discouraged)-->
        <!--<disable-game>DARK</disable-game>-->
    </compatibility>
    <categories>
        <category>PRAYER</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>INVALID</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>true</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>private</access>
    <tags>
        <tag>Spaghetti</tag>
    </tags>
    <resources>
        <resource>/resources</resource>
    </resources>
</manifest>